{

  "manifest_version": 2,
  "name": "Instagram Sidebar",
  "description": "Displays Instagram in a sidebar.",
  "version": "1.0",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "54.0a1"
    }
  },


  "sidebar_action": {
    "default_icon": "icons/instagram.png",
    "default_title" : "Instagram",
    "default_panel": "sidebar/panel.html"
  }
  
}